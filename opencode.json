{
    "project": "Antigravity Zero H2",
    "version": "2.0",
    "ai": {
        "provider": "openai",
        "model": "gpt-5-mini",
        "temperature": 0.2,
        "max_tokens": 8192,
        "reasoning_effort": "medium"
    },
    "context": {
        "auto_index": true,
        "ignore_patterns": [
            "node_modules",
            ".git",
            "dist"
        ],
        "max_files": 50
    },
    "features": {
        "inline_suggestions": true,
        "auto_build": false
    }
}