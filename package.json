{
  "name": "antigravity-zero",
  "version": "1.0.0-opus",
  "description": "Jarvis Desktop Controller â€” AI-powered Ubuntu automation with OpenClaw Bot hub",
  "type": "module",
  "main": "src/jarvis/orchestrator.mjs",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "jarvis": "node src/jarvis/swarm-server.mjs",
    "bot": "node src/jarvis/bridge/telegram-controller.mjs",
    "demo": "node scripts/demo.mjs",
    "test": "node tests/smoke.mjs",
    "smoke": "node tests/smoke.mjs",
    "bridge:test": "node src/jarvis/bridge/openclaw-bridge.mjs --test",
    "health": "curl -sf http://localhost:4566/_localstack/health && curl -sf http://localhost:9005/minio/health/live && curl -sf http://localhost:6333/healthz && echo ' ALL HEALTHY'",
    "infra:up": "cd infra && docker compose up -d && docker compose up minio-init",
    "infra:down": "cd infra && docker compose down",
    "infra:reset": "cd infra && docker compose down -v && docker compose up -d",
    "tf:init": "cd infra/terraform && terraform init -input=false",
    "tf:plan": "bash scripts/tf-plan.sh",
    "tf:apply": "bash scripts/tf-apply.sh",
    "dashboard": "xdg-open http://localhost:7777",
    "test:super": "node tests/super-test.mjs",
    "scheduler": "node src/jarvis/scheduler/autonomous-agent.mjs --start",
    "scheduler:status": "node src/jarvis/scheduler/autonomous-agent.mjs --status"
  },
  "author": "Will-dev (Sovereign)",
  "license": "MIT",
  "dependencies": {
    "@qdrant/js-client-rest": "^1.16.2",
    "axios": "^1.13.5",
    "dotenv": "^17.2.4",
    "openai": "^6.21.0",
    "playwright": "^1.58.2",
    "telegraf": "^4.16.3"
  },
  "keywords": [
    "jarvis",
    "desktop-automation",
    "ai-agent",
    "swarm",
    "terraform",
    "localstack",
    "minio",
    "playwright",
    "aider",
    "openclaw"
  ]
}