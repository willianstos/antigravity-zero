{
  "name": "antigravity-zero",
  "version": "1.5.0-sovereign-elite",
  "description": "Sovereign Swarm Commander — Elite Pro HVAC-R Assistant 2026",
  "type": "module",
  "main": "src/jarvis/orchestrator.mjs",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "start": "node src/jarvis/orchestrator.mjs",
    "bot": "pkill -f 'node src/core/telegram-bot.js' || true && node src/core/telegram-bot.js",
    "swarm": "node src/jarvis/swarm-server.mjs",
    "health-check": "bash scripts/health-check.sh",
    "self-heal": "npm run health-check",
    "infra:up": "cd infra && sudo docker compose up -d",
    "infra:status": "node scripts/bootstrap-stability.mjs",
    "audit": "node tests/super-test.mjs",
    "test": "node --check src/jarvis/orchestrator.mjs && node --check src/jarvis/browser/llm-engine.mjs && echo '✅ Syntax OK'",
    "maestro:status": "curl -s http://localhost:11434/api/tags 2>/dev/null | head -c 500 || echo '⚠️ Ollama offline'",
    "prod": "npm run infra:up && npm run swarm & npm run bot",
    "tf:apply": "cd infra/terraform && terraform apply -auto-approve",
    "prepare": "cp scripts/pre-commit-hook.sh .git/hooks/pre-commit 2>/dev/null && chmod +x .git/hooks/pre-commit 2>/dev/null || true"
  },
  "author": "Will-dev (Sovereign)",
  "license": "MIT",
  "dependencies": {
    "@qdrant/js-client-rest": "^1.16.2",
    "axios": "^1.13.5",
    "dotenv": "^17.2.4",
    "openai": "^6.21.0",
    "playwright": "^1.58.2",
    "telegraf": "^4.16.3"
  },
  "keywords": [
    "jarvis",
    "hvac-r",
    "ai-agent",
    "swarm",
    "daikin",
    "pmoc",
    "local-first-ai",
    "ollama",
    "qwen",
    "openclaw"
  ]
}